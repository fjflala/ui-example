{
  "extends": "airbnb",
  "plugins": [
    "jest",
    "react",
    "import"
  ],
  "env": {
    "jest": true
  },
  "rules": {
    "strict": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/destructuring-assignment": [0, "always"],
    "jsx-a11y/label-has-for": false
  },
  "parser": "babel-eslint"
}